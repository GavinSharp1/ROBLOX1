for i = 1, 25, .5 do
for j = 1, 25, .5 do
for k = 1, 25, .5 do

i=i +4
k=k + 4
j=j + math.cos(k)

p = Instance.new("Part")
p.CFrame = CFrame.new(Vector3.new(i,j,i))
p.Size = Vector3.new(1,1,1)
p.Anchored = true
p.BrickColor = BrickColor.Random()
p.formFactor = "Symmetric"
p.Parent = game.Workspace
wait()
end
end
end
